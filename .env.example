# Laravel application configuration file

# Application name
app_name="Laravel"

# Application environment
app_env="local"

# Application key (generate a new one using `php artisan key:generate`)
app_key=""

# Debug mode (set to true to enable detailed error messages)
app_debug="true"

# Application timezone (set to your desired timezone)
app_timezone="UTC"

# Application URL (set to your application's base URL)
app_url="http://localhost"

# Application locale (set to your desired locale)
app_locale="en"

# Application fallback locale (set to your desired fallback locale)
app_fallback_locale="en"

# Application faker locale (set to your desired faker locale)
app_f
